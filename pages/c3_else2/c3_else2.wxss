.tabs, .tab1, .tab2, .tab3, .tab4{
  width: 100%;
  height: 100%;
  text-align: center;
}


.divide {
  width: 750rpx;
  height: 55rpx;
  text-align: center;
  color: #aaa;
  font-size: 24rpx;
  margin-top: 50rpx;
  /* margin-top: 20rpx; */
  /* background-color: #f7cac9; */
}


.list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin-top: 130rpx;
}

.item {
  margin-left: 50rpx;
  margin-bottom: 50rpx;
  background-color: #91a8d0;
  box-shadow: 5rpx 5rpx 12rpx rgb(184, 183, 183);
  height: 390rpx;
  width: 300rpx;
  display: flex;
  flex-direction: column;
  color: #fff;
  font-family: Arial, Helvetica, sans-serif;
}

.item-name {
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  line-height: 1.4;
  font-weight: 500;
}

.big {
  font-size: 30rpx;
}

.small {
  font-size: 25rpx;
}


.item2 {
  background-color: #f7cac9;
  color: #7B6564;
}

.item3 {
  background-color: #BAD17D;
  color: #ffffff;
}

.item4 {
  
  background-color: #BCBDC1;
  color: #5E5E60;
}

.img-container {
  width: 300rpx;
  height: 300rpx;
  overflow: hidden;
}

.item image {
  width: 280rpx;
  height: 280rpx;
  margin-top: 10rpx;
}

.footer {
  text-align: center;
  height: 40rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-style: italic;
  font-size: 26rpx;
  color: rgb(87, 87, 87);
}


