}
 
.title {
  width: 100%;
  height: 88rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
  /* position:fixed; */
  /* top: 50; */
  /* margin-top: 50rpx; */
  z-index: 100;
}
 
.titleSel1, .titleSel2, .titleSel3 , .titleSel4{
  width: 24%;
  font-size: 28rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-weight: bold;
}

.titleSel1 {
  color: #91a8d0;
}

.titleSel2 {
  color: #7B6564;
}

.titleSel3 {
  color: #2c5732;
}

.titleSel4 {
  color: #5E5E60;
}


.headerLineSel1, .headerLineSel2, .headerLineSel3, .headerLineSel4 {
  height: 6rpx;
  width: 40rpx;
  position: relative;
  margin-top: 10rpx;
  
}
 
.headerLineSel1 {
  background: #91a8d0;
}

.headerLineSel2 {
  background: #7B6564;
}

.headerLineSel3 {
  background: #2c5732;
}

.headerLineSel4 {
  background: #5E5E60;
}

 
.headerLineUnsel {
  background: #fff;
